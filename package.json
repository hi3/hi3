{
  "name": "hi3",
  "version": "1.0.0",
  "description": "Hugo & Tailwind CSS 3.0",
  "main": "index.js",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.3.3"
  },
  "scripts": {
    "start": "concurrently npm:start:*",
    "start:tw": "tailwindcss -c ./themes/hi3/tailwind.config.js -i ./themes/hi3/assets/css/main.css -o ./themes/hi3/assets/css/style.css --watch",
    "start:hugo": "hugo --minify server --disableFastRender --bind $(eth0ip) --baseURL=http://$(eth0ip)",
    "build": "concurrently npm:build:*",
    "build:tw": "tailwindcss -c ./themes/hi3/tailwind.config.js -i ./themes/hi3/assets/css/main.css -o ./themes/hi3/assets/css/style.css",
    "build:hugo": "hugo --minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rpinz/hi3.git"
  },
  "keywords": [
    "hugo",
    "tailwind",
    "tailwindcss"
  ],
  "author": "RonPinz",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/rpinz/hi3/issues"
  },
  "homepage": "https://github.com/rpinz/hi3/#readme",
  "dependencies": {}
}
